    <LinearLayout android:orientation="horizontal" style="@style/MsimSetting_Oper_Item">
        <TextView android:id="@id/operandnum_cardname" android:text="@string/wifi_ssid" style="@style/MsimSettingText" />
        <View android:layout_width="@dimen/msim_settings_line_size" android:layout_height="@dimen/msim_setting_line_height" style="@style/MsimSetting_Oper_LineView" />
