    </PreferenceScreen>
    <PreferenceScreen android:title="@string/manual" android:key="manual">
        <intent android:action="android.settings.SHOW_MANUAL" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/legal_information" android:key="container" android:fragment="com.android.settings_ex.LegalInformationFragment" />
